{
    "eula": "",
    "vendor": "SAP",
    "license": "",
    "id": "com.sap.sample.export.xls",
    "version": "0.0.27",
    "name": "Export XLS",
    "newInstancePrefix": "ExportXLS",
    "description": "A sample widget that allows users to export XLS files.",
    "imports": ["table", "input-controls", "planning", "datasource"],
    "webcomponents": [
        {
            "kind": "main",
            "tag": "com-sap-sample-export-xls",
            "url": "/exportXLS.js",
            "integrity": "",
            "ignoreIntegrity": true
        }
    ],
    "properties": {},
    "methods": {
        "exportData": {
            "returnType": "any",
            "description": "Exports the data to an Excel file and initiates the download.",
            "parameters": [
                {
                    "name": "resultSet",
                    "type": "string[]", 
                    "description": "The data to be exported to the Excel file."
                }
            ]
        }
    },
    "events": {
        "onFileExport": {
            "description": "Triggered when a file has been successfully exported."
        }
    }
}
